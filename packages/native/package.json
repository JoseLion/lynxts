    "@lynxts/core": "workspace:^",
    "@react-native/metro-config": "^0.72.9",
    "@stackbuilders/assertive-ts": "^1.3.1",
    "@testing-library/react-native": "^12.1.3",
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.18",
