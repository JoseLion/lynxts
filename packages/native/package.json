{
  "name": "@lynxts/native",
  "version": "0.0.0",
  "description": "Lynx.ts native package",
  "repository": "git@github.com:JoseLion/lynxts.git",
  "homepage": "https://joselion.gitbook.io/lynx.ts/",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react.js",
    "react native",
    "form",
    "typescript",
    "validation",
    "yup",
    "zod"
  ],
  "type": "module",
  "sideEffects": false,
  "source": "./src/main.ts",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "unpkg": "./dist/main.umd.cjs",
  "types": "./dist/main.d.ts",
  "react-native": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "react-native": "./dist/main.js",
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./dist/",
    "./src/",
    "./package.json"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build",
    "check": "yarn compile && yarn test",
    "compile": "tsc -p tsconfig.json",
    "docs": "typedoc",
    "release": "semantic-release",
    "test": "NODE_ENV=test jest",
    "test:silent": "DEBUG_PRINT_LIMIT=0 yarn test",
    "test:verbose": "DEBUG_PRINT_LIMIT=1000000 yarn test"
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "react-fast-compare": "^3.2.2"
  },
  "devDependencies": {
    "@assertive-ts/core": "^2.1.0",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@lynxts/core": "workspace:^",
    "@react-native/babel-preset": "^0.74.85",
    "@react-native/metro-config": "^0.74.85",
    "@testing-library/react-native": "^12.5.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@types/sinon": "^17.0.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-native": "^0.74.3",
    "react-test-renderer": "^18.3.1",
    "semantic-release": "^24.0.0",
    "semantic-release-yarn": "^3.0.2",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "yup": "^1.4.0"
  },
  "peerDependencies": {
    "@lynxts/core": ">=1.0.0",
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "peerDependenciesMeta": {
    "@lynxts/core": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-native": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
