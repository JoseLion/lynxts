{
  "name": "@lynxts/core",
  "version": "0.0.0",
  "description": "Lynx.ts core package",
  "repository": "git@github.com:JoseLion/lynxts.git",
  "homepage": "https://joselion.gitbook.io/lynx.ts/",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react.js",
    "form",
    "typescript",
    "validation",
    "yup",
    "zod"
  ],
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "yarn compile && yarn test",
    "compile": "tsc -p tsconfig.json",
    "docs": "typedoc",
    "release": "semantic-release",
    "test": "NODE_ENV=test mocha",
    "test:silent": "DEBUG_PRINT_LIMIT=0 yarn test",
    "test:verbose": "DEBUG_PRINT_LIMIT=1000000 yarn test"
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "dot-prop-immutable": "^2.1.1",
    "scheduler": "^0.23.2",
    "use-context-selector": "^2.0.0"
  },
  "devDependencies": {
    "@assertive-ts/core": "^2.1.0",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/mocha": "^10.0.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sinon": "^17.0.3",
    "expect-type": "^0.19.0",
    "jsdom": "^24.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-release": "^24.0.0",
    "semantic-release-yarn": "^3.0.2",
    "sinon": "^18.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "optionalDependencies": {
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "yup": ">=1.0.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "yup": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
