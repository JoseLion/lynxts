{
  "name": "@lynxts/web",
  "version": "0.0.0",
  "description": "Lynx.ts web package",
  "repository": "https://github.com/JoseLion/lynxts.git",
  "homepage": "https://joselion.gitbook.io/lynx.ts/",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react.js",
    "react dom",
    "form",
    "typescript",
    "validation",
    "yup",
    "zod"
  ],
  "type": "module",
  "sideEffects": false,
  "source": "./src/main.ts",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./dist/",
    "./src/",
    "./package.json"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build",
    "check": "yarn compile && yarn test --run",
    "compile": "tsc -p tsconfig.json",
    "docs": "typedoc",
    "release": "semantic-release",
    "test": "vitest",
    "test:silent": "DEBUG_PRINT_LIMIT=0 yarn test",
    "test:verbose": "DEBUG_PRINT_LIMIT=1000000 yarn test"
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "react-fast-compare": "^3.2.2"
  },
  "devDependencies": {
    "@assertive-ts/core": "^2.1.0",
    "@assertive-ts/sinon": "^1.0.0",
    "@lynxts/core": "workspace:^",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sinon": "^17.0.3",
    "happy-dom": "^14.12.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-release": "^24.0.0",
    "semantic-release-yarn": "^3.0.2",
    "sinon": "^18.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "yup": "^1.4.0"
  },
  "peerDependencies": {
    "@lynxts/core": ">=1.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@lynxts/core": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
