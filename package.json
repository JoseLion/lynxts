{
  "name": "lynxts",
  "version": "0.0.0",
  "description": "The Lynx.ts workspace",
  "repository": "git@github.com:JoseLion/lynxts.git",
  "homepage": "https://joselion.gitbook.io/lynx.ts/",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com>",
  "engines": {
    "node": ">=16"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "build": "turbo build",
    "check": "turbo check && yarn lint",
    "compile": "turbo compile",
    "docs": "turbo docs",
    "lint": "eslint . --report-unused-disable-directives",
    "test": "turbo test",
    "release": "turbo release --concurrency=1",
    "turbo": "turbo"
  },
  "devDependencies": {
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "semantic-release": "^21.0.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "turbo": "^1.10.12",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typedoc-plugin-merge-modules": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
