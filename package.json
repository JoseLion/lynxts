{
  "name": "lynxts",
  "version": "0.0.0",
  "description": "The Lynx.ts workspace",
  "repository": "git@github.com:JoseLion/lynxts.git",
  "homepage": "https://joselion.gitbook.io/lynx.ts/",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com>",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.2.1",
  "scripts": {
    "build": "turbo build",
    "check": "turbo check && yarn lint",
    "compile": "turbo compile",
    "docs": "turbo docs",
    "lint": "eslint .",
    "test": "turbo test",
    "release": "turbo release --concurrency=1",
    "turbo": "turbo"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@stylistic/eslint-plugin": "^2.2.2",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.12.10",
    "eslint": "^9.5.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-extra-rules": "^0.0.0-development",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.3.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-sonarjs": "^1.0.3",
    "globals": "^15.6.0",
    "semantic-release": "^23.0.8",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "turbo": "^1.13.3",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^4.0.1",
    "typedoc-plugin-merge-modules": "^5.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  }
}
